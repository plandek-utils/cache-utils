{
  "name": "@plandek-utils/cache-utils",
  "version": "0.2.0",
  "exports": "./src/mod.ts",
  "tasks": {
    "release": "npx light-release && deno fmt",
    "outdated": "deno run -rA jsr:@check/deps",
    "nuke": "deno clean && rm -rf node_modules tmp_cov coverage.lcov html_cov && deno install --allow-scripts",
    "ci": "NODE_ENV=test deno test --coverage=tmp_cov --allow-sys --allow-env src && deno coverage --lcov --output=coverage.lcov tmp_cov",
    "test": "NODE_ENV=test deno test --coverage=tmp_cov --allow-sys --allow-env src",
    "lcov": "deno coverage --lcov --output=coverage.lcov tmp_cov",
    "html": "genhtml -o html_cov coverage.lcov"
  },
  "license": "MIT",
  "imports": {
    "@apollo/utils.keyvaluecache": "npm:@apollo/utils.keyvaluecache@3.1.0",
    "@eturino/ioredis-del-by-pattern": "npm:@eturino/ioredis-del-by-pattern@2.0.1",
    "@plandek-utils/plain-object": "jsr:@plandek-utils/plain-object@0.2.3",
    "@std/assert": "jsr:@std/assert@1.0.7",
    "@std/expect": "jsr:@std/expect@1.0.7",
    "@std/testing": "jsr:@std/testing@1.0.4",
    "@types/ioredis-mock": "npm:@types/ioredis-mock@8.2.5",
    "ioredis": "npm:ioredis@5.4.1",
    "ioredis-mock": "npm:ioredis-mock@8.9.0"
  },
  "nodeModulesDir": "auto",
  "fmt": {
    "lineWidth": 120,
    "include": [
      "src/",
      ".release-notes/"
    ]
  },
  "lint": {
    "include": [
      "src/",
      ".release-notes/"
    ]
  },
  "publish": {
    "include": [
      "LICENSE",
      "README.md",
      "src/**/*.ts"
    ],
    "exclude": [
      "src/__tests__"
    ]
  }
}